{%
set classes = [
'paragraph',
'paragraph--type--' ~ paragraph.bundle|clean_class,
view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}
<div{{ attributes.addClass(classes) }}>

	<h2 class="video-title">{{ content.field_title }}</h2>
	{{ content.field_divider }}
	<div class="video-embed">{{ content.field_video_url }}</div>
</div>